<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <context-param>
    <param-name>imageUploads</param-name>
    <param-value>assets/images/post</param-value>
  </context-param>
  <context-param>
    <param-name>imageUploadsCover</param-name>
    <param-value>\assets\images\banner</param-value>
  </context-param>
  <context-param>
    <param-name>imageUploadsDisplay</param-name>
    <param-value>\assets\images\profile</param-value>
  </context-param>

  <error-page>
    <!-- Missing resource -->
    <error-code>404</error-code>
    <location>/WEB-INF/error/error-404.html</location>
  </error-page>
  <error-page>
    <!-- Uncaught exception -->
    <error-code>500</error-code>
    <location>/WEB-INF/error/error-505.html</location>
  </error-page>

</web-app>
